{
  "name": "keepalive-react-router",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Route-Level Keep-Alive for React Router",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/keepalive-react-router.git"
  },
  "keywords": [
    "keepalive",
    "react-router"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "prepare": "simple-git-hooks",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint -e",
    "lint": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c --no-push -t --all -x \"npm run changelog\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6.4",
    "react-transition-preset": ">=3"
  },
  "peerDependenciesMeta": {
    "react-transition-preset": {
      "optional": true
    }
  },
  "dependencies": {
    "context-state": "^4.0.1",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@rsbuild/plugin-react": "^1.3.5",
    "@rslib/core": "^0.11.1",
    "@types/react": "^18.3.23",
    "bumpp": "^10.2.2",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "lint-staged": "^16.1.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "simple-git-hooks": "^2.13.1",
    "typescript": "5.8.3",
    "vitepress": "^1.6.3"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
